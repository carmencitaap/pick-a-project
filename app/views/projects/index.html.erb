<h1> Projects </h1>

<% if can? :create, Project %>
    <%= button_to "Crear Proyecto", new_project_path, method: :get %>
<% end %>


<% @projects.each do |project| %>
   <div> <%= project.title %> </div>
   <% if can? :destroy, Project %>
        <%= button_to "Eliminar", project_path(project), method: :delete %>
   <% end %>
<% end %>